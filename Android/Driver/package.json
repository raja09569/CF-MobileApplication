{
  "name": "com.camerounfacile.driver",
  "version": "1.0.0",
  "displayName": "CamerounFacile",
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "cordova-plugin-geolocation": {},
      "cordova-plugin-mauron85-background-geolocation": {
        "GOOGLE_PLAY_SERVICES_VERSION": "11+",
        "ANDROID_SUPPORT_LIBRARY_VERSION": "23+",
        "ICON": "@mipmap/icon",
        "SMALL_ICON": "@mipmap/icon",
        "ACCOUNT_NAME": "@string/app_name",
        "ACCOUNT_LABEL": "@string/app_name",
        "ACCOUNT_TYPE": "$PACKAGE_NAME.account",
        "CONTENT_AUTHORITY": "$PACKAGE_NAME"
      },
      "cordova-plugin-media": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-spinner-dialog": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-x-toast": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova.plugins.diagnostic": {},
      "cordova-plugin-datetimepicker": {},
      "uk.co.workingedge.phonegap.plugin.launchnavigator": {
        "OKHTTP_VERSION": "3.+"
      }
    }
  },
  "dependencies": {
    "cordova-android": "~7.0.0",
    "cordova-plugin-datetimepicker": "^1.1.2",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-geolocation": "^4.0.1",
    "cordova-plugin-mauron85-background-geolocation": "^2.3.5",
    "cordova-plugin-media": "^5.0.2",
    "cordova-plugin-network-information": "^2.0.1",
    "cordova-plugin-spinner-dialog": "^1.3.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-x-toast": "^2.6.2",
    "cordova.plugins.diagnostic": "^4.0.8",
    "uk.co.workingedge.phonegap.plugin.launchnavigator": "^4.2.1"
  }
}